{
  "name": "varlink",
  "displayName": "Varlink",
  "description": "Varlink language support",
  "version": "0.2.0",
  "publisher": "bachorp",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "varlink.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/bachorp/varlink-vscode.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "varlink",
        "aliases": [
          "Varlink",
          "varlink"
        ],
        "extensions": [
          ".varlink"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./varlink_mono.png",
          "dark": "./varlink_mono.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "varlink",
        "scopeName": "source.varlink",
        "path": "./varlink.tmLanguage.json"
      }
    ]
  }
}